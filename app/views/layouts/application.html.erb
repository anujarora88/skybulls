<!DOCTYPE html>
<html>
<head>
  <title>SkybullsRails</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if !flash.empty? %>
    <div id="flash">
      <% flash.keys.each do |k| %>
          <div class="<%= k %>">
            <%= flash[k] %>
          </div>
      <% end %>
    </div>
<% end %>
<%= yield %>

</body>
</html>
